<launch>
    <param name="use_sim_time" value="false"/>

    <arg name="namespace" default="wyf_ws_1"/>
    <group ns="$(arg namespace)">

    <!-- 启动nlink_parser驱动包 -->
    <node pkg="nlink_parser" type="linktrack" name="linktrack0" output="screen">
        <param name="port_name" value="/dev/ttyACM1" />
        <param name="baud_rate" value="921600" />
    </node>
    
    <!-- 启动获取UWB位置 -->
     <node name="uwb_position" pkg="uwb_position" type="uwb_position.py" output="screen" respawn="false"/> 

  <!-- <arg name="gui" default="false" />

  <param name="robot_description" textfile="$(find yhs_chassis_description)/urdf/autocube.urdf" />
  <param name="use_gui" value="$(arg gui)"/>
 
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find yhs_chassis_description)/rviz/urdf.rviz" /> -->

    <node pkg="yhs_can_control" type="yhs_can_control_node" name="yhs_can_control_node" output="screen">

	<param name="odom_frame" value="odom"/>
    <param name="base_link_frame" value="base_link"/>
    <param name="tfUsed" value="false"/>

    </node>
    
	<node pkg="serial_imu" name="HI226" type="serial_imu" output="screen" />

    <node pkg="tf2_ros" type="static_transform_publisher" name="imu_broadcaster" args="0.0 0.0 0.0 0 0 0 base_link imu_link" />

</group> 
</launch>